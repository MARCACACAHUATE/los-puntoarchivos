
syntax enable

set number relativenumber 
set background=dark
set laststatus=0
set smartindent
set tabstop=4
set showtabline=4
set autoindent
set clipboard=unnamedplus
set encoding=utf-8
set fileencoding=utf-8
set splitbelow
set splitright
set mouse=a

filetype on
filetype indent on
filetype plugin on
